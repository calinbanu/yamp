{
  "name": "yamp-app",
  "productName": "yamp",
  "version": "1.0.0",
  "description": "Yet Another Map Parser Electron Application",
  "keywords": [
    "file",
    "map",
    "parser",
    "compiler"
  ],
  "author": "Banu Calin",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "electron-snowpack build",
    "clean": "electron-snowpack clean",
    "dev": "electron-snowpack dev",
    "dist": "electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "open": "open dist/mac/${npm_package_name}.app",
    "try": "run-s clean build dist:dir open"
  },
  "dependencies": {
    "electron-snowpack": "*"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "22.10.5",
    "npm-run-all": "^4.1.5"
  },
  "build": {
    "extends": "electron-snowpack/config/electron-builder.js"
  }
}